{
  "private": true,
  "name": "zuord-monorepo",
  "scripts": {
    "prerelase:do" : "npm version prerelease --preid alpha",
    "publish:do" : "npm run prerelase:do && npm publish --access public --tag alpha",
    "publish:type" : "cd type && npm run publish:do",
    "publish:util" : "cd util && npm run publish:do",
    "publish:api" : "cd api && npm run publish:do",
    "publish:repo" : "npm run publish:api && npm run publish:util && npm run publish:type"
  },
  "dependencies": {
    "@zuord/type": "0.3.0-alpha"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.3",
    "@types/node": "^24.0.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.1",
    "tslib": "^2.8.1"
  }
}
