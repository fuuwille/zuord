{
    "name": "@zuord/bundle",
    "version": "1.0.0-alpha.8",
    "dependencies": {
        "@zuord/core": "3.0.0-alpha.28",
        "@zuord/trait": "3.0.0-alpha.14",
        "@zuord/type": "2.0.0-alpha.18",
        "@zuord/util": "4.0.0-alpha.13",
        "zuord": "5.0.0-alpha.21"
    },
    "exports": {
        ".": {
            "import": "./lib/index.js",
            "types": "./lib/index.d.ts"
        },
        "./type": {
            "import": "./type/index.js",
            "types": "./type/index.d.ts"
        },
        "./core": {
            "import": "./core/index.js",
            "types": "./core/index.d.ts"
        },
        "./trait": {
            "import": "./trait/index.js",
            "types": "./trait/index.d.ts"
        },
        "./util": {
            "import": "./util/index.js",
            "types": "./util/index.d.ts"
        }
    },
    "scripts": {
        "extract:lib": "node extractor.js node_modules/zuord/dist $/lib",
        "extract:type": "node extractor.js node_modules/@zuord/type/dist $/type",
        "extract:core": "node extractor.js node_modules/@zuord/core/dist $/core",
        "extract:trait": "node extractor.js node_modules/@zuord/trait/dist $/trait",
        "extract:util": "node extractor.js node_modules/@zuord/util/dist $/util",
        "extract": "npm run extract:lib && npm run extract:type && npm run extract:core && npm run extract:trait && npm run extract:util",
        "build": "rimraf $ lib type core trait util && npm run extract && rollup -c"
    }
}