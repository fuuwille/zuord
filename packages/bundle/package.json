{
    "name": "@zuord/bundle",
    "dependencies": {
        "@zuord/core": "3.0.0-alpha.15",
        "@zuord/trait": "3.0.0-alpha.10",
        "@zuord/type": "2.0.0-alpha.10",
        "@zuord/util": "4.0.0-alpha.10",
        "zuord": "5.0.0-alpha.11"
    },
    "scripts": {
        "cpy:lib": "cpy 'node_modules/zuord/dist/**/*' src/lib",
        "cpy:type": "cpy 'node_modules/@zuord/type/dist/**/*' src/type",
        "cpy:core": "cpy 'node_modules/@zuord/core/dist/**/*' src/core",
        "cpy:trait": "cpy 'node_modules/@zuord/trait/dist/**/*' src/trait",
        "cpy:util": "cpy 'node_modules/@zuord/util/dist/**/*' src/util",
        "cpy": "npm run cpy:lib && npm run cpy:type && npm run cpy:core && npm run cpy:trait && npm run cpy:util"
    }
}